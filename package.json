{
  "name": "blog-site-api",
  "version": "0.0.0",
  "description": "CRUD operations to the blog site database",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/serve.ts",
